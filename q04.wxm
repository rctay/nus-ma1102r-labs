f(n,x):=sum(((2*k-1)^3) * x^(2*k-1),k,1,n);
fa(x):=x*(x+1)*(x^4+22*x^2+1)/((x-1)^4*(x+1)^4);
fb(x):=x*(x^2+1)*(x^4+22*x^2+1)/((x-1)^4*(x+1)^4);
fc(x):=x^2*(x+1)*(x^4+22*x^2+1)/((x-1)^4*(x+1)^4);
fd(x):=x^2*(x^2+1)*(x^4+22*x^2+1)/((x-1)^4*(x+1)^4);

try(n, x):=(
    printf(true, "fn(x)=~a~%", f(1000, x)),
    printf(true, "fa(x)=~a~%", fa(x)),
    printf(true, "fb(x)=~a~%", fb(x)),
    printf(true, "fc(x)=~a~%", fc(x)),
    printf(true, "fd(x)=~a~%", fd(x)));
try(100, 0.3)